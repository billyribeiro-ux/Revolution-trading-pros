{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "Revolution Trading Pros - Enterprise Trading Platform",
    "keywords": ["laravel", "framework", "trading", "fintech"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "intervention/image": "^3.11",
        "jenssegers/agent": "^2.6",
        "kalnoy/nestedset": "^6.0",
        "kornrunner/blurhash": "^1.2",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.29",
        "laravel/octane": "^2.6",
        "laravel/pulse": "^1.3",
        "laravel/reverb": "^1.4",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "predis/predis": "^3.2",
        "sentry/sentry-laravel": "^4.10",
        "spatie/laravel-permission": "^6.23",
        "spatie/laravel-sluggable": "^3.7",
        "stripe/stripe-php": "^16.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.8",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "@php artisan octane:install --server=frankenphp --no-interaction",
            "@php artisan horizon:install",
            "@php artisan pulse:install"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74,#34d399\" \"php artisan octane:start --watch\" \"php artisan horizon\" \"php artisan pail --timeout=0\" \"php artisan reverb:start\" --names=octane,horizon,logs,reverb --kill-others"
        ],
        "dev:standard": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" --names=server,queue,logs --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "octane": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan octane:start --server=frankenphp --host=0.0.0.0 --port=8000"
        ],
        "octane:watch": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan octane:start --server=frankenphp --watch"
        ],
        "horizon": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan horizon"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
