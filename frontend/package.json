{
	"name": "revolution-svelte",
	"version": "2.0.0",
	"description": "A SvelteKit project for Revolution.",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"dev:fast": "vite dev",
		"build": "vite build",
		"build:cloudflare": "DEPLOY_TARGET=cloudflare vite build",
		"build:static": "DEPLOY_TARGET=static vite build",
		"preview": "vite preview",
		"check": "NODE_OPTIONS='--max-old-space-size=4096' svelte-check --threshold error",
		"check:all": "NODE_OPTIONS='--max-old-space-size=4096' svelte-check",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "playwright test",
		"test:ui": "playwright test --ui",
		"test:headed": "playwright test --headed",
		"test:e2e": "playwright test tests/e2e",
		"test:e2e:smoke": "playwright test tests/e2e/smoke --project=chromium",
		"test:e2e:auth": "playwright test tests/e2e/auth --project=chromium",
		"test:e2e:trading": "playwright test tests/e2e/trading-room --project=chromium",
		"test:e2e:checkout": "playwright test tests/e2e/checkout --project=chromium",
		"test:e2e:api": "playwright test tests/e2e/api --project=api",
		"test:e2e:chromium": "playwright test tests/e2e --project=chromium",
		"test:e2e:firefox": "playwright test tests/e2e --project=firefox",
		"test:e2e:webkit": "playwright test tests/e2e --project=webkit",
		"test:e2e:mobile": "playwright test tests/e2e --project=mobile-chrome --project=mobile-safari",
		"test:e2e:all": "playwright test tests/e2e --project=chromium --project=firefox --project=webkit",
		"test:e2e:debug": "E2E_DEBUG=true playwright test tests/e2e --headed --debug",
		"test:e2e:report": "playwright show-report",
		"test:unit": "vitest",
		"test:unit:ui": "vitest --ui",
		"deploy:cloudflare": "npm run build:cloudflare && wrangler pages deploy .svelte-kit/cloudflare",
		"deploy:preview": "npm run build:cloudflare && wrangler pages deploy .svelte-kit/cloudflare --branch preview",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@formkit/auto-animate": "^0.9.0",
		"@fortawesome/fontawesome-svg-core": "^7.1.0",
		"@fortawesome/free-brands-svg-icons": "^7.1.0",
		"@fortawesome/free-solid-svg-icons": "^7.1.0",
		"@fortawesome/svelte-fontawesome": "^0.2.4",
		"@stripe/stripe-js": "8.6.4",
		"@tabler/icons-svelte": "^3.36.1",
		"@threlte/core": "^8.3.1",
		"@threlte/extras": "^9.7.1",
		"@vimeo/player": "^2.30.1",
		"blurhash": "^2.0.5",
		"canvas-confetti": "^1.9.4",
		"d3": "^7.9.0",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-svelte": "^8.6.0",
		"gsap": "^3.14.2",
		"heroicons-svelte": "^2.0.2",
		"isomorphic-dompurify": "^2.35.0",
		"lightweight-charts": "^5.1.0",
		"lottie-web": "^5.13.0",
		"three": "^0.182.0",
		"tus-js-client": "^4.3.1",
		"typed.js": "^3.0.0",
		"vite-plugin-devtools-json": "1.0.0",
		"vivus": "^0.4.6",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^5.0.0",
		"@internationalized/date": "3.10.1",
		"@lucide/svelte": "0.561.0",
		"@opentelemetry/api": "^1.9.0",
		"@playwright/test": "^1.58.0",
		"@storybook/addon-a11y": "^10.2.0",
		"@storybook/addon-docs": "^10.2.0",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-vitest": "^10.2.0",
		"@storybook/sveltekit": "^10.2.0",
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/adapter-static": "3.0.10",
		"@sveltejs/kit": "2.50.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/canvas-confetti": "^1.9.0",
		"@types/d3": "^7.4.3",
		"@types/node": "^25",
		"@types/three": "0.182.0",
		"@vitest/browser-playwright": "^4.0.18",
		"@vitest/coverage-v8": "^4.0.18",
		"bits-ui": "2.15.5",
		"clsx": "2.1.1",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "^10.2.0",
		"eslint-plugin-svelte": "^3.14.0",
		"playwright": "^1.58.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"storybook": "^10.2.0",
		"super-sitemap": "1.0.6",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"svelte-inspect-value": "0.10.0",
		"svelte-render-scan": "1.1.0",
		"tailwind-merge": "3.4.0",
		"tailwind-variants": "3.2.2",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"uuid": "^13.0.0",
		"vite": "^7.3.1",
		"vite-plugin-compression": "^0.5.1",
		"vitest": "^4.0.18",
		"wrangler": "4.60.0"
	}
}
