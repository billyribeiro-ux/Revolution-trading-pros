{
	"name": "revolution-svelte",
	"version": "2.0.0",
	"description": "A SvelteKit project for Revolution.",
	"private": true,
	"type": "module",
	"packageManager": "npm@10.8.2",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "svelte-kit sync && vite dev",
		"dev:fast": "vite dev",
		"build": "svelte-kit sync && vite build",
		"build:cloudflare": "DEPLOY_TARGET=cloudflare vite build",
		"build:static": "DEPLOY_TARGET=static vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --threshold error",
		"check:all": "svelte-kit sync && svelte-check",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "playwright test",
		"test:ui": "playwright test --ui",
		"test:headed": "playwright test --headed",
		"test:e2e": "playwright test tests/e2e",
		"test:e2e:smoke": "playwright test tests/e2e/smoke --project=chromium",
		"test:e2e:auth": "playwright test tests/e2e/auth --project=chromium",
		"test:e2e:trading": "playwright test tests/e2e/trading-room --project=chromium",
		"test:e2e:checkout": "playwright test tests/e2e/checkout --project=chromium",
		"test:e2e:api": "playwright test tests/e2e/api --project=api",
		"test:e2e:chromium": "playwright test tests/e2e --project=chromium",
		"test:e2e:firefox": "playwright test tests/e2e --project=firefox",
		"test:e2e:webkit": "playwright test tests/e2e --project=webkit",
		"test:e2e:mobile": "playwright test tests/e2e --project=mobile-chrome --project=mobile-safari",
		"test:e2e:all": "playwright test tests/e2e --project=chromium --project=firefox --project=webkit",
		"test:e2e:debug": "E2E_DEBUG=true playwright test tests/e2e --headed --debug",
		"test:e2e:report": "playwright show-report",
		"test:unit": "vitest",
		"test:unit:ui": "vitest --ui",
		"deploy:cloudflare": "npm run build:cloudflare && wrangler pages deploy .svelte-kit/cloudflare",
		"deploy:preview": "npm run build:cloudflare && wrangler pages deploy .svelte-kit/cloudflare --branch preview",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@formkit/auto-animate": "^0.9.0",
		"@fortawesome/fontawesome-svg-core": "^7.1.0",
		"@fortawesome/free-brands-svg-icons": "^7.1.0",
		"@fortawesome/free-solid-svg-icons": "^7.1.0",
		"@fortawesome/svelte-fontawesome": "^0.2.4",
		"@tabler/icons-svelte": "^3.36.1",
		"@threlte/core": "^8.3.1",
		"@threlte/extras": "^9.7.1",
		"@vimeo/player": "^2.30.1",
		"blurhash": "^2.0.5",
		"canvas-confetti": "^1.9.4",
		"d3": "^7.9.0",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-svelte": "^8.6.0",
		"gsap": "^3.14.1",
		"heroicons-svelte": "^2.0.2",
		"isomorphic-dompurify": "^2.34.0",
		"lightweight-charts": "^5.0.9",
		"lottie-web": "^5.13.0",
		"three": "^0.182.0",
		"tus-js-client": "^4.3.1",
		"typed.js": "^2.1.0",
		"vivus": "^0.4.6",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^5.0.0",
		"@opentelemetry/api": "^1.9.0",
		"@playwright/test": "^1.57.0",
		"@storybook/addon-a11y": "^10.1.11",
		"@storybook/addon-docs": "^10.1.11",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-vitest": "^10.1.11",
		"@storybook/sveltekit": "^10.1.11",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/adapter-static": "3.0.10",
		"@sveltejs/kit": "2.50.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/canvas-confetti": "^1.9.0",
		"@types/d3": "^7.4.3",
		"@types/node": "^20",
		"@types/three": "0.182.0",
		"@types/tus-js-client": "^1.8.0",
		"@types/uuid": "^10.0.0",
		"@vitest/browser-playwright": "^4.0.15",
		"@vitest/coverage-v8": "^4.0.15",
		"dotenv": "^16.6.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "^10.1.11",
		"eslint-plugin-svelte": "^3.13.1",
		"playwright": "^1.57.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"storybook": "^10.1.11",
		"svelte": "^5.45.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.49.0",
		"uuid": "^11.1.0",
		"vite": "^6.4.1",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.15",
		"wrangler": "4.59.2"
	}
}
