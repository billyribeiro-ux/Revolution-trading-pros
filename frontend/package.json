{
	"name": "revolution-svelte",
	"version": "2.0.0",
	"description": "A SvelteKit project for Revolution.",
	"private": true,
	"type": "module",
	"packageManager": "npm@10.9.2",
	"engines": {
		"node": ">=20.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"dev:fast": "vite dev",
		"build": "vite build",
		"build:cloudflare": "DEPLOY_TARGET=cloudflare vite build",
		"build:static": "DEPLOY_TARGET=static vite build",
		"preview": "vite preview",
		"preview:component": "node scripts/preview-component.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:all": "NODE_OPTIONS='--max-old-space-size=4096' svelte-check",
		"check:types": "tsc --noEmit",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "NODE_OPTIONS='--max-old-space-size=4096' eslint .",
		"lint:fix": "NODE_OPTIONS='--max-old-space-size=4096' eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "vitest",
		"test:ui": "playwright test --ui",
		"test:headed": "playwright test --headed",
		"test:unit": "vitest run",
		"test:unit:ui": "vitest --ui",
		"test:unit:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:smoke": "playwright test tests/e2e/smoke --project=chromium",
		"test:e2e:auth": "playwright test tests/e2e/auth --project=chromium",
		"test:e2e:trading": "playwright test tests/e2e/trading-room --project=chromium",
		"test:e2e:checkout": "playwright test tests/e2e/checkout --project=chromium",
		"test:e2e:api": "playwright test tests/e2e/api --project=api",
		"test:e2e:chromium": "playwright test tests/e2e --project=chromium",
		"test:e2e:firefox": "playwright test tests/e2e --project=firefox",
		"test:e2e:webkit": "playwright test tests/e2e --project=webkit",
		"test:e2e:mobile": "playwright test tests/e2e --project=mobile-chrome --project=mobile-safari",
		"test:e2e:all": "playwright test tests/e2e --project=chromium --project=firefox --project=webkit",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:e2e:report": "playwright show-report",
		"test:a11y": "pa11y-ci",
		"test:visual": "playwright test tests/visual",
		"test:ci": "npm run test:unit && npm run test:e2e",
		"test:blog-editor": "playwright test --project=blog-editor",
		"test:blog-editor:firefox": "playwright test --project=blog-editor-firefox",
		"test:blog-editor:mobile": "playwright test --project=blog-editor-mobile",
		"test:blog-editor:all": "playwright test --project=blog-editor --project=blog-editor-firefox --project=blog-editor-mobile",
		"test:blog-editor:visual": "playwright test --project=visual-regression",
		"test:blog-editor:a11y": "playwright test --project=accessibility",
		"analyze": "vite-bundle-visualizer",
		"deploy:cloudflare": "npm run build:cloudflare && wrangler pages deploy .svelte-kit/cloudflare",
		"deploy:preview": "npm run build:cloudflare && wrangler pages deploy .svelte-kit/cloudflare --branch preview"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.74.0",
		"@formkit/auto-animate": "^0.9.0",
		"@fortawesome/fontawesome-svg-core": "^7.1.0",
		"@fortawesome/free-brands-svg-icons": "^7.1.0",
		"@fortawesome/free-solid-svg-icons": "^7.1.0",
		"@fortawesome/svelte-fontawesome": "^0.2.4",
		"@sentry/sveltekit": "^10.38.0",
		"@stripe/stripe-js": "^8.7.0",
		"@tabler/icons-svelte-runes": "^3.36.1",
		"@tanstack/svelte-virtual": "^3.13.18",
		"@threlte/core": "^8.3.1",
		"@threlte/extras": "^9.7.1",
		"@vimeo/player": "^2.30.1",
		"blurhash": "^2.0.5",
		"canvas-confetti": "^1.9.4",
		"d3": "^7.9.0",
		"diff-match-patch": "^1.0.5",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-svelte": "^8.6.0",
		"gsap": "^3.14.2",
		"html2canvas": "^1.4.1",
		"isomorphic-dompurify": "^2.36.0",
		"lightweight-charts": "^5.1.0",
		"lottie-web": "^5.13.0",
		"svelte-click-to-source": "*",
		"three": "^0.182.0",
		"tus-js-client": "^4.3.1",
		"typed.js": "^3.0.0",
		"vivus": "^0.4.6",
		"web-vitals": "^5.1.0",
		"y-indexeddb": "^9.0.12",
		"y-protocols": "^1.0.7",
		"y-websocket": "^3.0.0",
		"yjs": "^13.6.29",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.11.0",
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.563.1",
		"@opentelemetry/api": "^1.9.0",
		"@playwright/test": "^1.58.2",
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@sveltejs/vite-plugin-svelte-inspector": "^5.0.2",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@types/canvas-confetti": "^1.9.0",
		"@types/d3": "^7.4.3",
		"@types/d3-array": "^3.2.2",
		"@types/d3-axis": "^3.0.6",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-scale-chromatic": "^3.1.0",
		"@types/d3-selection": "^3.0.11",
		"@types/d3-shape": "^3.1.8",
		"@types/diff-match-patch": "^1.0.36",
		"@types/node": "^25.2.2",
		"@types/three": "0.182.0",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"@vitest/browser-playwright": "^4.0.18",
		"@vitest/coverage-v8": "^4.0.18",
		"bits-ui": "^2.11.7",
		"clsx": "2.1.1",
		"d3-array": "^3.2.4",
		"d3-axis": "^3.0.0",
		"d3-scale": "^4.0.2",
		"d3-scale-chromatic": "^3.1.0",
		"d3-selection": "^3.0.0",
		"d3-shape": "^3.2.0",
		"dotenv": "^17.2.4",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^14.0.0",
		"magic-string": "^0.30.21",
		"pa11y-ci": "^4.0.1",
		"playwright": "^1.58.2",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"super-sitemap": "^1.0.7",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"svelte-inspect-value": "0.10.0",
		"svelte-render-scan": "1.1.0",
		"tailwind-merge": "3.4.0",
		"tailwind-variants": "3.2.2",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"uuid": "^13.0.0",
		"vite": "^7.3.1",
		"vite-bundle-visualizer": "^1.2.1",
		"vite-plugin-devtools-json": "1.0.0",
		"vitest": "^4.0.18",
		"wrangler": "^4.63.0"
	}
}
