        function clearCart() {
            // Make an AJAX request to trigger the clear_woocommerce_cart() function
            console.log("Hello clear cart 123");
            jQuery.ajax({
                url: 'https://revolutiontradingpros.com/wp-admin/admin-ajax.php',
                type: 'POST',
                data: {
                    'action': 'clear_woocommerce_cart'
                },
                success: function (response) {
                    console.log("Hello");
                    console.log(response, "wow");
                    window.location.reload(); // Reload the page
                }
            });
        }


		function register() {
            // Make an AJAX request to trigger the clear_woocommerce_cart() function
            console.log("Hello register");
			var form = document.getElementById("logForm");
    		form.style.display = "none";
			var site = document.getElementById("site");
			site.style.display = "none";
			var regform = document.getElementById("regform");
			regform.style.display = "block";
			var register_head = document.getElementById("register_head");
			register_head.style.display = "block";
			var element = document.querySelector('.page-id-194.elementor-page-194 .woocommerce .wpmc-steps-wrapper .wpmc-step-item>#checkout_login');
			element.classList.remove('custom-styles');
			element.style.display = 'block';
			
			
		}
		function logon() {
			var regforms = document.getElementById("regform");
			regforms.style.display = "none";
			var register_heads = document.getElementById("register_head");
			register_heads.style.display = "none";
			var forms = document.getElementById("logForm");
    		forms.style.display = "block";
			var sites = document.getElementById("site");
			sites.style.display = "flex";
			var element = document.querySelector('.page-id-194.elementor-page-194 .woocommerce .wpmc-steps-wrapper .wpmc-step-item>#checkout_login');
			element.classList.remove('custom-styles');
			element.style.display = 'flex';
		}

    </script>
    
