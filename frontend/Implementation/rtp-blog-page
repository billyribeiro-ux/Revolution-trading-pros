function clearCart() {
            // Make an AJAX request to trigger the clear_woocommerce_cart() function
            console.log("Hello clear cart 123 footer");
            jQuery.ajax({
                url: 'https://revolutiontradingpros.com/wp-admin/admin-ajax.php',
                type: 'POST',
                data: {
                    'action': 'clear_woocommerce_cart'
                },
                success: function (response) {
                    console.log("Hello");
                    console.log(response, "wow");
                    window.location.reload(); // Reload the page
                }
            });
        }
	
	

