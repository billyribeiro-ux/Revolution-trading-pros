// Function to replace SVG with image
function replaceSVGWithImage() {
    jQuery('.ha-pg-author svg').each(function() {
        var newImg = jQuery('<img src="https://revolutiontradingpros.com/wp-content/uploads/2023/06/Billy-Adjusted-2-300x300.png.webp" width="50px" height="50px" style="float:left;margin-top:-6px">');
        jQuery(this).replaceWith(newImg);
    });

    document.querySelectorAll('.ha-pg-meta-wrap a').forEach(link => {
        link.removeAttribute('href');
    });
}

// Run the replacement function initially for existing content
replaceSVGWithImage();

// Check if the load more button exists
const loadmoreButton = document.querySelector('.eicon-loading');

if (loadmoreButton) {
    // Create a new MutationObserver instance
    const observer = new MutationObserver(mutations => {
        mutations.forEach(mutation => {
            if (mutation.attributeName === 'style') {
                const displayValue = window.getComputedStyle(loadmoreButton).display;
                if (displayValue === 'none') {
                    replaceSVGWithImage();
                }
            }
        });
    });

    // Define the observer options
    const configForSVGtoPP = {
        attributes: true,       // Observe attribute changes
        attributeFilter: ['style']  // Only observe changes to the 'style' attribute
    };

    // Start observing the target node
    observer.observe(loadmoreButton, configForSVGtoPP);
} else {
    console.log('No load more button found.');
}

	
	$(document).on('click', '#elementor-menu-cart__toggle_button', function(e) {
		console.log('clicked');
		e.preventDefault();
		event.stopImmediatePropagation();
		window.location.href = '/cart';
	});
</script>

        <script type="text/javascript">
    jQuery(document).ready(function($) {
        var $firstItem = $('.account-secondry-menu li').first();

        // List of IDs where first item should be active
        var activeIds = [
            'explosive-swings',
            'swing-trading',
            'small-accounts',
            'day-trading'
        ];

        var pathname = window.location.pathname;
        var searchParams = new URLSearchParams(window.location.search);
        var idParam = searchParams.get('id');

        // If on the my-membership page AND ID matches one in our list
        if (pathname === '/my-account/my-membership/' && activeIds.includes(idParam)) {
            $firstItem.addClass('is-active');
        } else {
            $firstItem.removeClass('is-active');
        }
    });
    </script>
			<script>
				;
				(function($, w) {
					'use strict';
					let $window = $(w);

					$(document).ready(function() {

						let isEnable = "";
						let isEnableLazyMove = "";
						let speed = isEnableLazyMove ? '0.7' : '0.2';

						if( !isEnable ) {
							return;
						}

						if (typeof haCursor == 'undefined' || haCursor == null) {
							initiateHaCursorObject(speed);
						}

						setTimeout(function() {
							let targetCursor = $('.ha-cursor');
							if (targetCursor) {
								if (!isEnable) {
									$('body').removeClass('hm-init-default-cursor-none');
									$('.ha-cursor').addClass('ha-init-hide');
								} else {
									$('body').addClass('hm-init-default-cursor-none');
									$('.ha-cursor').removeClass('ha-init-hide');
								}
							}
						}, 500);

					});

				}(jQuery, window));
