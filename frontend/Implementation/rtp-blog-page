    <script>
    jQuery(document).ready(function($) {
        // Remove loading indicator
        jQuery("#loading").remove();
        console.log("Hello world");

        var cartcount = 0;

        // AJAX request to get cart item count
        $.ajax({
            url: custom_ajax_object.ajax_url,
            type: 'POST',
            data: { action: 'custom_get_cart_items_count' },
            success: function(response) {
                console.log(response, ' mr');
                cartcount = response;
                console.log('Cart items count: ' + response);
            },
            error: function(errorThrown) {
                console.log('AJAX Error: ' + errorThrown);
            }
        });

        // Toggle cart visibility based on cart count
        $(document).on('click', '#elementor-menu-cart__toggle_button', function(e) {
            e.preventDefault();
            console.log("Hello tog", cartcount);

            if (cartcount == 0) {
                $('body').after('<div id="error-message" class="woocommerce-error">The cart is empty.</div>');
                $('.elementor-menu-cart__container.elementor-lightbox').hide();
                $('#error-message').css({
                    'position': 'absolute',
                    'top': '142px',
                    'right': '30px',
                    'z-index': '1',
                    'border-radius': '10px',
                    'border-top-color': '#223967'
                });
            } else {
                console.log("count 1 ");
                $('.elementor-menu-cart__container.elementor-lightbox').show();
                $('#error-message').remove();
            }
        });

        // Subscription status wrapping
        $('td.subscription-status').each(function () {
            var statusText = $(this).text().trim();
            $(this).html('<span>' + statusText + '</span>');
        });

        // Address modification and class assignment
        var originalText = $(".woocommerce-edit-address address").html();
        var lines = originalText.split("<br>");
        $(".woocommerce-edit-address address").empty();

        for (var i = 0; i < lines.length; i++) {
            var line = lines[i].trim();
            if (line !== "") {
                $(".woocommerce-edit-address address").append("<div class='address-" + i + "'>" + line + "</div>");
            }
        }

        // Add user image to address box
        $('section.elementor-section.elementor-top-section .woocommerce-MyAccount-content .u-column1.col-1.woocommerce-Address').attr('id', 'addressBox');
        $('#addressBox').prepend("<div class='userImg'></div>");

        // Handle other DOM manipulations like wrapping divs, adding classes, etc.
        $('.subscription-status span').each(function () {
            var labelVal = $(this).text();
            if (labelVal === 'On hold') {
                $(this).addClass('onHold');
            } else if (labelVal === 'Active') {
                $(this).addClass('active');
            } else if (labelVal === 'Pending Cancellation') {
                $(this).addClass('cancelled');
            }
        });

        // Handle click events for .dot elements
        $('.dot').attr('href', 'javascript:void(0)').click(function () {
            var index = $('.dot').index(this);
            $('.specific-div:eq(' + index + ')').toggle();
        });
    });
