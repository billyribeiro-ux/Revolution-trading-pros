		function logon() {
			var regforms = document.getElementById("regform");
			regforms.style.display = "none";
			var register_heads = document.getElementById("register_head");
			register_heads.style.display = "none";
			var forms = document.getElementById("logForm");
    		forms.style.display = "block";
			var sites = document.getElementById("site");
			sites.style.display = "flex";
