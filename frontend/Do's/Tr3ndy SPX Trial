				<div class="elementor-element elementor-element-dc642ed elementor-hidden-desktop elementor-hidden-tablet elementor-widget elementor-widget-html" data-id="dc642ed" data-element_type="widget" data-widget_type="html.default">
				<div class="elementor-widget-container">
			<script>
window.onload = function(){ 

    
    var pageURL = document.URL;
    var fb = document.getElementsByClassName('fb-wrap');
    var fbButtons = fb.length;
    var tweet = document.getElementsByClassName('tweet-wrap');
    var tweetButtons = tweet.length;
    var share = document.getElementsByClassName('share-wrap');
    var shareButtons = share.length;
    var pageLink = document.getElementsByClassName("share-page-link");
    var pageLinkWindows = pageLink.length;

    function shareOnFacebook(){
      var navUrl = 'https://www.facebook.com/sharer/sharer.php?u=' + pageURL +'?utm_source=facebook&utm_medium=sales_share';
      window.open(navUrl , 'popUpWindow','height=300,width=700,left=50,top=50,resizable=yes,scrollbars=no,toolbar=no,menubar=no,location=no,directories=no, status=no');
    }
    
    
    function shareOnTwitter() {
      var navUrl =
        'https://twitter.com/intent/tweet?text=' + pageURL +'?utm_source=twitter&utm_medium=sales_share';
      window.open(navUrl, 'popUpWindow','height=300,width=700,left=50,top=50,resizable=yes,scrollbars=no,toolbar=no,menubar=no,location=no,directories=no, status=yes');
    }
    
    function copyToClipboard() {
      var inputc = document.body.appendChild(document.createElement("input"));
      inputc.value = window.location.href+'?utm_source=copy_link&utm_medium=sales_share';
      inputc.select();
      document.execCommand('copy');
      inputc.parentNode.removeChild(inputc);
      
      for (var i = 0; i < shareButtons; i++) {
          share[i].style.pointerEvents = "none";
        }
      
      for (var i = 0; i < pageLinkWindows; i++) {
          pageLink[i].innerHTML += 'URL Copied to Clipboard';
          pageLink[i].classList.add('hideMe');
        }
      
      setTimeout(function(){
          for (var i = 0; i < pageLinkWindows; i++) {
              pageLink[i].classList.remove('hideMe');
              pageLink[i].innerHTML = '';
              share.style = '';
            }
            for (var i = 0; i < shareButtons; i++) {
              share[i].style = '';
              document.activeElement.blur();
            }
        }, 5000);
  
    }
    
    if(tweet){
        for (var i = 0; i < tweetButtons; i++) {
          tweet[i].addEventListener('click', shareOnTwitter, false);
        }
    }
    
    if(fb){
        for (var i = 0; i < fbButtons; i++) {
          fb[i].addEventListener('click', shareOnFacebook, false);
        }
    }
    
    if(share){
        for (var i = 0; i < shareButtons; i++) {
          share[i].addEventListener('click', copyToClipboard, false);
        }
    }
    
};


</script>
<div class="share-icons-wrap">
    <ul class="page-share-list">
    <li class="share-title">SHARE THIS TRIAL</li>
    <li class="fb-wrap"></li>
    <li class="tweet-wrap"></li>
    <li class="share-wrap"></li>
    </ul>
    <div class="share-page-link"></div>
</div>		</div>
				</div>
					</div>
				</div>
				</div>
					</div>
				</div>
				</div>
				</div>
