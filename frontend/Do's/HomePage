			<script>

jQuery(document).ready(function($) {

setTimeout(function() {
     if(!jQuery('body').hasClass('elementor-editor-active')){
        
        jQuery('.eael-testimonial-item').each(function() {
        
        var $this = jQuery(this);
        var traderNum = jQuery(this).attr('data-swiper-slide-index');
        var traderURL = 'https://www.simplertrading.com/traders/';
        var john = 'john-carter';
        var raghee = 'raghee-horner';
        var henry = 'henry-gambell';
        var bruce = 'bruce-marshall';
        var chris = 'chris-brecher';
        var danielle = 'danielle-shay';
        var tg = 'tg-watkins';
        var taylor = 'taylor-horton';
        var sam = 'sam-shames';
        var david = 'david-starr';
        var kody = 'kody-ashmore';
        var allison = 'allison-ostrander';
        var mike = 'mike-teeto';
        var jonathan = 'jonathan-mckeever';
        var a = traderURL;

    
        if (traderNum === '0') {
                var a = jQuery('<a/>').attr('href', traderURL + john);
            }
        if (traderNum === '1') {
            var a = jQuery('<a/>').attr('href', traderURL + raghee);
        }
        if (traderNum === '2') {
            var a = jQuery('<a/>').attr('href', traderURL + henry);
        }
        if (traderNum === '3') {
            var a = jQuery('<a/>').attr('href', traderURL + bruce);
        }
        if (traderNum === '4') {
            var a = jQuery('<a/>').attr('href', traderURL + danielle);
        }
        if (traderNum === '5') {
            var a = jQuery('<a/>').attr('href', traderURL + kody);
        }
        if (traderNum === '6') {
            var a = jQuery('<a/>').attr('href', traderURL + allison);
        }
        if (traderNum === '7') {
            var a = jQuery('<a/>').attr('href', traderURL + tg);
        }
        if (traderNum === '8') {
            var a = jQuery('<a/>').attr('href', traderURL + taylor);
        }
        if (traderNum === '9') {
            var a = jQuery('<a/>').attr('href', traderURL + sam);
        }
        if (traderNum === '10') {
            var a = jQuery('<a/>').attr('href', traderURL + david);
        }
        if (traderNum === '11') {
            var a = jQuery('<a/>').attr('href', traderURL + jonathan);
        }
        if (traderNum === '12') {
            var a = jQuery('<a/>').attr('href', traderURL + chris);
        }
        if (traderNum === '13') {
            var a = jQuery('<a/>').attr('href', traderURL + mike);
        }
        
        $this.children().children('.eael-testimonial-image').wrap(a);
    });
        };
}, 4000);

});

</script>

<script>
document.addEventListener('DOMContentLoaded', function() {
  // Simple desktop check: no touch and screen width > 768px
  const isDesktop = window.innerWidth > 768 && !('ontouchstart' in window);

  if (isDesktop) {
    const popupLinks = document.querySelectorAll('.popuplink');

    popupLinks.forEach(function(link) {
      link.addEventListener('click', function(e) {
        e.preventDefault();

        window.open(
          this.href,
          'popupWindow',
          'width=450,height=800,scrollbars=yes,resizable=yes,toolbar=no,menubar=no,status=no,location=no'
        );
      });
    });
  }
});
</script>		</div>
