			<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.4/moment.min.js" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script>
document.addEventListener("DOMContentLoaded", () => {
  const sheetId = '11DUUlg7F1mP95Oqb0KhwqpLKiXwrMwuGWLXfn0i08rU';
  const apiKey = 'AIzaSyDgCJLYbKTfxY6ZER9eGMXAhHKN2Qf4ans';
  /*const url = 'https://sheets.googleapis.com/v4/spreadsheets/${sheetId}/values/Sheet2!A2:F100?key=${apiKey}';*/
  const url = 'https://sheets.googleapis.com/v4/spreadsheets/11DUUlg7F1mP95Oqb0KhwqpLKiXwrMwuGWLXfn0i08rU/values/Sheet2!A2:F100?key=AIzaSyDgCJLYbKTfxY6ZER9eGMXAhHKN2Qf4ans';
  
  let showTicker = false;
  
    fetch(url)
      .then(response => response.json())
      .then(data => {
        const tickerContainer = document.getElementById('ticker-container');
        const tickerContent = document.getElementById('ticker-content');
        const currentDate = moment(); // Get current date and time using Moment.js
      
      // Testiing only
      // const currentDate = 1753917921;
        
const events = data.values.slice(1).filter(value => {
        const eventDate = moment(value[0], 'M/D/YYYY');
        const eventStartTime = moment(value[0]+' '+value[1], 'M/D/YYYY h:mm A');
        const eventEndTime = moment(value[0]+' '+value[2], 'M/D/YYYY h:mm A');
          return moment(eventEndTime).isSameOrAfter(currentDate, 'minute');
        }).slice(0, 13);
        

        if (events.length > 0) {
          document.getElementById('ticker').style.display = 'block';
        } 


        function appendData() {
          events.forEach(value => {
            const eventName = value[3];
            const eventDate = moment(value[0]).format('MMMM Do');
            const eventTime1 = value[1].replace(":00 ", "");
            const eventTime2 = value[2].replace(":00 ", "");
            const combinedTime = combineStrings(eventTime1, eventTime2);
            // const eventHost = value[3];
            const eventLink = value[5];
            
            const eventElement = document.createElement('span');
            const linkElement = document.createElement('a');

            if (eventLink != '' && eventLink != undefined) {
                linkElement.href = eventLink;
                linkElement.textContent = `${eventName} - ${eventDate} @ ${combinedTime}`;
                eventElement.appendChild(linkElement);
            } else {
                eventElement.textContent = `${eventName} - ${eventDate} @ ${combinedTime}`;
            }
            
            // eventElement.style.marginRight = '50px'; // Add some spacing between events

            tickerContent.appendChild(eventElement);
          });
        }

        appendData();
        appendData();

        let eventElements = Array.from(tickerContent.children); // Convert to an array
        let currentIndex = 0;
        let isAnimationPaused = false; // Track animation state
        let initialX = null;
        let scrollLeft = null;

        const backButton = document.getElementById('back-button');
        const forwardButton = document.getElementById('forward-button');

        backButton.addEventListener('click', navigateBack);
        forwardButton.addEventListener('click', navigateForward);

        tickerContainer.addEventListener('mousedown', startDrag);
        tickerContainer.addEventListener('mousemove', drag);
        tickerContainer.addEventListener('mouseup', endDrag);
        tickerContainer.addEventListener('mouseleave', endDrag);

        function navigateBack() {
          pauseAnimation();
          currentIndex = currentIndex === 0 ? eventElements.length - 1 : currentIndex - 1;
          tickerContainer.scrollLeft = eventElements[currentIndex].offsetLeft;
          resumeAnimation();
        }

        function navigateForward() {
          pauseAnimation();
          currentIndex = currentIndex === eventElements.length - 1 ? 0 : currentIndex + 1;
          tickerContainer.scrollLeft = eventElements[currentIndex].offsetLeft;
          resumeAnimation();
        }

        function pauseAnimation() {
          if (!isAnimationPaused) {
            tickerContent.style.animationPlayState = 'paused';
            isAnimationPaused = true;
          }
        }

        function resumeAnimation() {
          if (isAnimationPaused) {
            tickerContent.style.animationPlayState = 'running';
            isAnimationPaused = false;
          }
        }

        function startDrag(e) {
          initialX = e.clientX;
          scrollLeft = tickerContainer.scrollLeft;
          tickerContainer.style.cursor = 'grabbing';
          pauseAnimation();
        }

        function drag(e) {
          if (initialX === null) {
            return;
          }

          const offsetX = e.clientX - initialX;
          tickerContainer.scrollLeft = scrollLeft - offsetX;
        }

        function endDrag() {
          initialX = null;
          tickerContainer.style.cursor = 'grab';
          resumeAnimation();
        }
      })
      .catch(error => console.error('Error:', error));
      
      var winWidth = jQuery('body').width();
      if(winWidth <= 768){
        jQuery('#ticker-container').width(winWidth - 5);
        }
        
    function combineStrings(string1, string2) {
      const hasAM1 = string1.includes('AM');
      const hasPM1 = string1.includes('PM');
      const hasAM2 = string2.includes('AM');
      const hasPM2 = string2.includes('PM');
    
      if ((hasAM1 && hasAM2) || (hasPM1 && hasPM2)) {
        string1 = string1.replace('AM', '').replace('PM', '');
      }
    
      return string1 + ' - ' + string2;
    }
})

  </script>		</div>
