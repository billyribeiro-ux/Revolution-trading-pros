			// Initial Data Layer Init
			var om_load_webfont = false;
			window.dataLayer = window.dataLayer || [];
			window.dataLayer.push({
				'event': 'dataLayer-initialized',
				'user_id': 94190,
				'pageType': "Uncategorized",
				'membership_type': [],
				'content_author_id': "2",
				'content_author': "Taylor Letterman",
