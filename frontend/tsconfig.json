{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"types": ["svelte"],
		"moduleResolution": "bundler",
		"module": "ESNext",
		"target": "ESNext",
		"allowImportingTsExtensions": true,
		"skipLibCheck": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strict": false
	}
}
