{
  "name": "svelte-click-to-source",
  "version": "0.1.0",
  "description": "Alt+Click any element in dev mode to open its source in your editor",
  "author": "Welber Ribeiro",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["svelte", "sveltekit", "vite", "developer-tools", "click-to-source"],
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "svelte": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "magic-string": "^0.30.0"
  }
}
